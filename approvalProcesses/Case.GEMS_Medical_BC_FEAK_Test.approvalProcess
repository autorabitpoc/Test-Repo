    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>DTU_Queue</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>GEMS Medical Benefit Coding FEAK Test Approval</label>
        <name>GEMS_Medical_Benefit_Coding_FEAK_Test_Approval</name>
    </approvalStep>
    <emailTemplate>GEMS_Templates/GEMS_Medical_BC_FEAK_TEST_Approval</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>Case.RecordType</field>
            <operation>equals</operation>

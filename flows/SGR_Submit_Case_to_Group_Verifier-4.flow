        </inputParameters>
    </actionCalls>
    <decisions>
        <name>Check_Reason_Value</name>
        <label>Check Reason Value</label>
        <locationX>391</locationX>
        <locationY>262</locationY>
        <defaultConnector>
            <targetReference>Error_Submit_Case_to_Group_Verifier</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Not BlueSTAR Fallout</defaultConnectorLabel>
        <rules>
            <name>BlueSTAR_Fallout</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>FoundCase.Reason</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>eligibility</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>FoundCase.Reason</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>transfer</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Success</targetReference>
            </connector>
            <label>BlueSTAR Fallout</label>
        </rules>
    </decisions>

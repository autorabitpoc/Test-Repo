    <actionCalls>
        <name>Update_Opportunity_Workflow_Activity</name>
        <label>Update Opportunity Workflow Activity</label>
        <locationX>1071</locationX>
        <locationY>86</locationY>
        <actionName>SGR_CaseReassignment</actionName>
        <actionType>apex</actionType>
        <inputParameters>

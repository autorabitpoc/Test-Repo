<apex:page standardController="Service_Request__c" extensions="LGNA_ServiceRequestSubmitCntlr" id="submitPage" standardStyleSheets="false" showHeader="false" sidebar="false" action="{!SubmitFunction}">
    <apex:pagemessages ></apex:pagemessages>
    <html xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
        <head>
        <script>
        function save(){
         sforce.one.navigateToURL('/{!Service_Request__c.Id}'); 
        }   
        function servclosed(){
         sforce.one.navigateToURL('/{!Service_Request__c.Id}'); 
        } 
        </script>
            <meta name="viewport" content="width=device-width, initial-scale=1"/>
            <apex:slds />
        </head>
<div class="demo-only" style="height: 640px;">
  <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
    <div class="slds-modal__container">
      <header class="slds-modal__header">
         <button class="slds-button slds-modal__close slds-button--icon-inverse" title="Close" onclick="servclosed()">
            X
            <span class="slds-assistive-text">Close</span>
            </button>
        <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">Cases</h2>
      </header>
      <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
                    <apex:outputText rendered="{!errorMessage!=null}">
                    {!errorMessage} 
                     <!--<a href="/one/one.app?source=aloha#/sObject/{!Case.Id}" >Cancel</a>-->
                    </apex:outputText>
                    <!------No Message ok button will dispaly--->
                      <apex:outputText rendered="{!errorMessage==null}">
                      Service Request Stage Submitted successfully.
                   <!--<a href="/one/one.app?source=aloha#/sObject/{!Case.Id}">Ok</a>-->
                    </apex:outputText>
     
      </div>
      <footer class="slds-modal__footer">
      <apex:form >
       <apex:commandbutton styleClass="slds-button slds-button--brand" rendered="true" value="Ok" onclick="save()"></apex:commandbutton>
        <apex:commandbutton styleClass="slds-button slds-button--brand" value="cancel" rendered="false" onclick="servclosed()"></apex:commandbutton>
        </apex:form> 
      </footer>
    </div>
  </section>
  <div class="slds-backdrop slds-backdrop_open"></div>
</div>
   </html>
</apex:page>
    <webLinks>
        <fullName>Billing_Profile_Cancellation</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Billing Profile Cancellation</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/21.0/connection.js&quot;)}
var ValidCase = &apos;{!Case.GEMS_Integration_Message__c}&apos;
if(ValidCase == null || ValidCase == &apos;&apos;){
    alert(&apos;Please Retrieve Account Structure first before clicking on this button&apos;);
}
else if(ValidCase != &apos;Account Structure details retrieved successfully from BlueSTAR&apos;){
    alert(&apos;Either Account Structure has been retrieved partially OR This case has been previously used for a cancellation. Please initiate a new Cancellation from Request Hub.&apos;);
}

else{
    window.open(&apos;/apex/BillingProfileCancellation?id={!Account.Id}&amp;Case={!Case.Id}&amp;selectedValue=Billing&apos;,&apos;_self&apos;);
}</url>
    </webLinks>

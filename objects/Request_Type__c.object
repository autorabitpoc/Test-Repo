    <webLinks>
        <fullName>Add_Edit_Proposal_NI_Type</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Add/Edit Proposal/NI Type</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/apex.js&quot;)}

var currName = sforce.connection.getUserInfo().userFullName;
var currId = sforce.connection.getUserInfo().userId;
var caseStatus = &apos;{!Case.Status}&apos;; 
var ownerName = &apos;{!Case.Owner_Name__c}&apos;;
var caseRT = &apos;{!Case.RecordType}&apos;;

var MembersId = sforce.connection.query(&quot;SELECT (SELECT UserOrGroupId FROM GroupMembers) FROM Group where DeveloperName = &apos;Network_Information&apos;&quot;);
records = MembersId.getArray(&quot;records&quot;);
var rLine = &apos;Record Line &apos; + records[0];


if(caseRT == &apos;Network Information&apos; &amp;&amp; caseStatus == &apos;Assigned&apos; &amp;&amp;
   ownerName == &apos;Network Information&apos; &amp;&amp; rLine.indexOf(currId) &lt; 0){
      alert(&apos;Only team members can make updates to a case assigned to the team.&apos;);
} else 
      if(caseRT == &apos;Network Information&apos; &amp;&amp; caseStatus == &apos;Assigned&apos; &amp;&amp;
         ownerName != &apos;Network Information&apos; &amp;&amp; rLine.indexOf(currId) &lt; 0){
            alert(&apos;Only team members can make updates to an assigned case.&apos;);
      } else {
        var url = window.location.href;
        var id = url.split(&quot;/&quot;);
        var strURL = &apos;/apex/AddEditProposalNIType?id=&apos; + id[id.length-1];
        window.open(strURL,&apos;_top&apos;);
      }</url>
    </webLinks>

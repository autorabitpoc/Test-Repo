    <webLinks>
        <fullName>Down_Vote</fullName>
        <availability>online</availability>
        <description>Vote down on enhancement request</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Down Vote</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/28.0/connection.js&apos;)}
{!REQUIRESCRIPT(&apos;/soap/ajax/28.0/apex.js&apos;)}


var userId = &apos;{!$User.Id}&apos;; 
var ideaId = &apos;{!Ideas__c.Id}&apos; ; 

var query= sforce.connection.query(&quot;Select Id From Vote__c where Vote__c.Voted_by__c = &apos;{!$User.Id}&apos; and Vote__c.Voted_on__c = &apos;{!Ideas__c.Id}&apos; &quot;); 

if(query.getArray(&quot;records&quot;).length == 0) { 
     var ideas= new sforce.SObject(&apos;Ideas__c&apos;); 
     ideas.id = &quot;{!Ideas__c.Id}&quot;; 
     ideas.Vote_Points__c = {!Ideas__c.Vote_Points__c} - 1; 

     var vote = new sforce.SObject(&apos;Vote__c&apos;); 
     vote.Voted_by__c = sforce.connection.getUserInfo().userId; 
     vote.Name = &quot;{!$User.FirstName} {!$User.LastName}&apos;s Vote On {!Ideas__c.Name}&quot;; 
     vote.Voted_on__c = ideaId; 

     var empty = sforce.apex.execute(&quot;IdeaClass&quot;, &quot;updateVoteNumber&quot;, {curIdea:ideas,        curVote:vote});

     location.reload(true); 
} 
else { 
    alert(&quot;You have already voted&quot;);
}</url>
    </webLinks>

        <linkType>javascript</linkType>
        <masterLabel>Send for Peer Review</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/connection.js&quot;)} 

var ScopeCodeObj = new sforce.SObject(&quot;GEMS_Pharmacy_Scope_Code__c&quot;); 
ScopeCodeObj.id = &apos;{!GEMS_Pharmacy_Scope_Code__c.Id}&apos;; /* Need Id field to update */
ScopeCodeObj.Status__c = &quot;Peer Review Pending&quot;; /* Change status */ 

if(&apos;{!GEMS_Pharmacy_Scope_Code__c.Peer_Reviewer__c}&apos; == &apos;&apos;) { 
alert(&apos;Please enter Peer Reviewer before Sending for Approval&apos;); 
} 
else { 

/* update method takes an array of Cases; init to 1 element - &apos;ScopeCodeObj&apos; */ 
var result = sforce.connection.update([ScopeCodeObj]); 

if (result[0].success == &apos;false&apos;) { 
console.long(result[0]);
alert(result[0].errors.message); 
} 
else { 

location.reload(true); /* redisplay the detail page */ 
} 
}</url>

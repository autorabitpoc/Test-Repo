    <validationRules>
        <fullName>GEMS_Group_Pay_Code</fullName>
        <active>true</active>
        <errorConditionFormula>IF(((ISPICKVAL(Case__r.HCSC_Division__c, &apos;NM&apos;) || ISPICKVAL(Case__r.HCSC_Division__c, &apos;IL&apos;))&amp;&amp; (LEN(Group_Pay_Code__c)&lt;2 || LEN(Group_Pay_Code__c)&gt;4)), true, false)</errorConditionFormula>
        <errorMessage>Group Pay Code: Expecting at least 2 characters. Maximum allowed length is 4 characters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GEMS_Group_Pay_Code_MT</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ((ISPICKVAL(Case__r.HCSC_Division__c, &apos;MT&apos;)) &amp;&amp;(Group_Pay_Code__c != &apos;XX&apos; &amp;&amp; Group_Pay_Code__c != &apos;00&apos;) ), true, false)</errorConditionFormula>
        <errorMessage>Group Pay Code: Expecting either &apos;XX&apos; or &apos;00&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GEMS_Group_Pay_Code_Required</fullName>
        <active>true</active>
        <errorConditionFormula>IF( (((ISPICKVAL(Case__r.HCSC_Division__c, &apos;IL&apos;)) || (ISPICKVAL(Case__r.HCSC_Division__c, &apos;NM&apos;)) || (ISPICKVAL(Case__r.HCSC_Division__c, &apos;MT&apos;)) ) &amp;&amp;((Group_Pay_Code__c == &apos;&apos;)|| (Group_Pay_Code__c == Null))), true, false)</errorConditionFormula>
        <errorMessage>Required fields are missing: [Group Pay Code]</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GEMS_Group_Pay_Code_Zeroes</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ((NOT(ISPICKVAL(Case__r.HCSC_Division__c, &apos;MT&apos;))) &amp;&amp;(Group_Pay_Code__c == &apos;000&apos; || Group_Pay_Code__c == &apos;00&apos; || Group_Pay_Code__c == &apos;0000&apos;) ), true, false)</errorConditionFormula>
        <errorMessage>Group Pay Code: &apos;00&apos; Or &apos;000&apos; Or &apos;0000&apos; is not allowed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GEMS_Prime_Plan_ID</fullName>
        <active>true</active>
        <errorConditionFormula>IF((LEN(Prime_Plan_ID__c)&lt;6 &amp;&amp; LEN(Prime_Plan_ID__c)&gt;0), true, false)</errorConditionFormula>
        <errorMessage>Prime Plan ID: Expecting a minimum of 6 numeric characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GEMS_Report_Format_Code</fullName>
        <active>true</active>
        <errorConditionFormula>IF(((ISPICKVAL(Case__r.HCSC_Division__c, &apos;MT&apos;))&amp;&amp; (ISPICKVAL(Report_Format_Code__c, &apos;&apos;))), true, false)</errorConditionFormula>
        <errorMessage>Please select Report Format Code</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GEMS_Rider_Code</fullName>
        <active>true</active>
        <errorConditionFormula>IF(((ISPICKVAL(Case__r.HCSC_Division__c, &apos;NM&apos;))&amp;&amp; (NOT(ISPICKVAL(Rider_Code__c, &apos;09&apos;) || ISPICKVAL(Rider_Code__c, &apos;10&apos;)))), true, false)</errorConditionFormula>
        <errorMessage>Rider Code: Expecting either &apos;09&apos; or &apos;10&apos; for NM Division</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GEMS_Rider_Code_Mandatory</fullName>

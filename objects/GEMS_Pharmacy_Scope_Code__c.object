        <errorConditionFormula>IF((((ISPICKVAL(Case__r.HCSC_Division__c, &apos;NM&apos;)) || (ISPICKVAL(Case__r.HCSC_Division__c, &apos;IL&apos;)) )&amp;&amp; (ISPICKVAL(Rider_Code__c, &apos;&apos;))), true, false)</errorConditionFormula>
        <errorMessage>Please select Rider Code</errorMessage>
    </validationRules>

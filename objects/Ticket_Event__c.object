    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Ticket_Event_Address_Required_Fields</fullName>
        <active>true</active>
        <description>Selecting upon Other from Event Venue picklist, Street, city, Zip Code should not be empty.</description>
        <errorConditionFormula>ISPICKVAL(Event_Venue_Name__c, &apos;Other&apos;) &amp;&amp; ( Event_City__c = null ||  Event_Street_Address__c = null ||  Event_Zip_Postal_Code__c = null)</errorConditionFormula>
        <errorMessage>Event Street, Event City, Event Zip Code are required when Event Venue is Other</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Event_Time</fullName>
        <active>true</active>
        <errorConditionFormula>AND(!(ISBLANK (Event_Time__c)), NOT(REGEX( Event_Time__c ,
&quot;(\\d([0-1]|[1-9]):\\d([0-1]|[1-60]) (AM|am|PM|pm))&quot;)))</errorConditionFormula>
        <errorDisplayField>Event_Time__c</errorDisplayField>
        <errorMessage>Event Time should be enter in format HH:MM PM/AM</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
    <webLinks>
        <fullName>New_Ticket_Event</fullName>
        <availability>online</availability>
        <description>Redirects user to visualforce page to create event and distribute tickets for that event</description>
        <displayType>massActionButton</displayType>
        <encodingKey>UTF-8</encodingKey>
        <height>600</height>
        <linkType>url</linkType>
        <masterLabel>New Ticket Event</masterLabel>
        <openType>noSidebar</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>/apex/TicketDistribution?acct={!Account.Id}</url>
    </webLinks>

    <validationRules>
        <fullName>Account_date_and_Renewal_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND((ISPICKVAL( BatchType__c , &apos;Batch&apos;) &amp;&amp; OR(ISPICKVAL( Division__c , &apos;IL&apos;) , ISPICKVAL( Division__c , &apos;TX&apos;),ISPICKVAL( Division__c , &apos;MT&apos;)) &amp;&amp; IF( Date_to_Account__c  &lt; ( DATE(year( Renewal_Effective_date__c ),month( Renewal_Effective_date__c ),1)-60), false, true)), ISCHANGED(Date_to_Account__c))</errorConditionFormula>
        <errorMessage>Date to Account must be at least 60 days before the Renewal Date</errorMessage>
    </validationRules>

        <fullName>Account_date_and_division_values</fullName>
        <active>true</active>
        <errorConditionFormula>AND((ISPICKVAL( BatchType__c , &apos;Batch&apos;) &amp;&amp; OR(ISPICKVAL( Division__c , &apos;NM&apos;) , ISPICKVAL( Division__c , &apos;OK&apos;)) &amp;&amp; IF(Date_to_Account__c  &gt; ( DATE(year( Renewal_Effective_date__c ),month( Renewal_Effective_date__c ),1)+60), false, true) &amp;&amp; IF( Date_to_Account__c  &lt; ( DATE(year( Renewal_Effective_date__c ),month( Renewal_Effective_date__c ),1)-85), false, true)), ISCHANGED(Date_to_Account__c))</errorConditionFormula>
        <errorMessage>Date to Account must be at least 85 days before the Renewal Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_to_Account_and_GA_and_Producer_date</fullName>
        <active>true</active>
        <description>when user enters Date to Account which is either less than Date to GA or Date to producer, this rule is activated</description>
        <errorConditionFormula>AND( ISCHANGED(Date_to_Account__c ), Date_to_Account__c  &lt;=  Date_to_GA__c  ||  Date_to_Account__c  &lt;=  Date_to_Producer__c)</errorConditionFormula>
        <errorMessage>Date to Account must come after the Date to GA and the Date to Producer</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_to_GA_and_Producer_and_Account_date</fullName>
        <active>true</active>
        <description>When user enters Date to GA which is greater than either Date to Producer or Date to Account, then this rule is activated.</description>
        <errorConditionFormula>AND(ISCHANGED(Date_to_GA__c), Date_to_GA__c  &gt;=  Date_to_Producer__c  ||  Date_to_GA__c  &gt;=  Date_to_Account__c)</errorConditionFormula>
        <errorMessage>Date to GA must be less than the Date to Producer and Date to Account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_to_producer_and_GA_and_account_date</fullName>
        <active>true</active>
        <description>when the user enters Date to producer less than Date to GA or greater than Date to Account, then this rule is invoked</description>
        <errorConditionFormula>AND(ISCHANGED(Date_to_Producer__c ), Date_to_Producer__c  &lt;=  Date_to_GA__c  ||  Date_to_Producer__c  &gt;=  Date_to_Account__c)</errorConditionFormula>
        <errorMessage>Date to Producer must be in between the Date to GA and Date to Account</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>

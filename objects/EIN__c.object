    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>CheckEINNumericLength9</fullName>
        <active>true</active>
        <errorConditionFormula>(NOT(ISNUMBER( Name )))  || 	
(LEN( Name ) &lt;&gt; 9)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>EIN has to be numeric value of length 9.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DoNotEndPrimary</fullName>
        <active>true</active>
        <description>This validation rule will not allow End Date to be updated for an EIN having Primary flag marked.</description>
        <errorConditionFormula>Primary__c == true &amp;&amp;
 NOT( ISNULL( End_Date__c ) )</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>Primary EIN cannot be ended. Please mark another EIN related to this Account as Primary before populating End Date for this EIN.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EIN_should_not_have_HCSC_value</fullName>
        <active>true</active>
        <errorConditionFormula>Name  =  TEXT(361236610)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Please do not use HCSC&apos;s EIN.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EndDateGreater</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISNEW() || ISPICKVAL(Change_Reason__c, &apos;ADD&apos;) 
|| ISPICKVAL(Change_Reason__c, &apos;CHG&apos;))
&amp;&amp; (End_Date__c &lt; Effective_Date__c || 
End_Date__c &lt; TODAY())</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>Please ensure that End Date is greater than Effective Date, and is not in the past.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EndedEIN</fullName>
        <active>true</active>
        <description>Do not allow changes to any field when an EIN is marked as Ended.</description>
        <errorConditionFormula>( NOT(ISBLANK( End_Date__c ))) &amp;&amp; (
 ISCHANGED(Account__c) ||
 ISCHANGED(EIN_Sequence_Number__c) ||
 ISCHANGED(Effective_Date__c) ||
 ISCHANGED(Street11__c) ||
 ISCHANGED(Street21__c) ||
 ISCHANGED(City1__c) ||
 ISCHANGED(State1__c) ||
 ISCHANGED(Zip_Code1__c) ||
 ISCHANGED(County_Code1__c) ||
 ISCHANGED(Country1__c) ||
 ISCHANGED(Location_Name__c) ||
 ISCHANGED( Name ) ||
 ISCHANGED(Primary__c))</errorConditionFormula>
        <errorMessage>Please do not change any field for EIN having End Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>InvalidEIN</fullName>
        <active>true</active>
        <description>Do not allow repeating number or consecutive number for EIN.</description>
        <errorConditionFormula>Name = &apos;111111111&apos; ||
 Name = &apos;222222222&apos; ||
 Name = &apos;333333333&apos; ||
 Name = &apos;444444444&apos; ||
 Name = &apos;555555555&apos; ||
 Name = &apos;666666666&apos; ||
 Name = &apos;777777777&apos; ||
 Name = &apos;888888888&apos; ||
 Name = &apos;999999999&apos; ||
 Name = &apos;000000000&apos; ||
 Name = &apos;123456789&apos; ||
 Name = &apos;012345678&apos; ||
 Name = &apos;098765432&apos; ||
 Name = &apos;987654321&apos;</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Please do not enter repeating or sequential numbers.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NoEndDateDuringAddChange</fullName>
        <active>true</active>
        <errorConditionFormula>( ISNEW() || ISPICKVAL(Change_Reason__c, &apos;ADD&apos;) 
|| ISPICKVAL(Change_Reason__c, &apos;CHG&apos;)
|| ISPICKVAL(Change_Reason__c, &apos;ERR&apos;)) 
&amp;&amp; NOT(ISNULL( End_Date__c ))</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>Please do not fill End Date until the EIN updates are transmitted to BlueStar.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NoNameChange</fullName>
        <active>true</active>
        <errorConditionFormula>(NOT(ISNEW())  &amp;&amp;  NOT(ISPICKVAL(Change_Reason__c, &apos;ADD&apos;))) &amp;&amp; 
ISCHANGED( Name )</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Employer Identification Number cannot be changed once transmitted to BlueStar.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SendingEIN</fullName>
        <active>true</active>
        <description>Do not allow changes to any field while transmission is in progress.</description>
        <errorConditionFormula>(  ISPICKVAL(Bluestar_Transfer_Status__c, &apos;Transfer In Transit&apos;)  || 
ISPICKVAL(Bluestar_Transfer_Status__c, &apos;Seq# updated&apos;)  ||
ISPICKVAL(Bluestar_Transfer_Status__c, &apos;Wait BS Confirm&apos;)  ||
ISPICKVAL(Bluestar_Transfer_Status__c, &apos;Seq# Requested&apos;)  ) &amp;&amp; (
 ISCHANGED(Account__c) ||
 ISCHANGED(Effective_Date__c) ||
 ISCHANGED(Street11__c) ||
 ISCHANGED(Street21__c) ||
 ISCHANGED(City1__c) ||
 ISCHANGED(State1__c) ||
 ISCHANGED(Zip_Code1__c) ||
 ISCHANGED(County_Code1__c) ||
 ISCHANGED(Country1__c) ||
 ISCHANGED(Location_Name__c) ||
 ISCHANGED(Name) ||
 ISCHANGED(End_Date__c) ||
 ISCHANGED(Primary__c))</errorConditionFormula>
        <errorMessage>This EIN is being transmitted to BlueStar. No changes are allowed at this point of time. If the issue persists for more than an hour, please reach out to IT support.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>

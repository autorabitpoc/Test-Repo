         IF ( Contact__c &lt;&gt; &quot;&quot;, &quot;Internal&quot;, 
           IF(Prospect__c&lt;&gt;&quot;&quot;, Specific_Prospect_Contact__r.FirstName +Specific_Prospect_Contact__r.LastName ,

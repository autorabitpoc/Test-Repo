        <active>true</active>
        <description>For Charity Tickets a Charity Contact is Required.</description>
        <errorConditionFormula>(AND(ISPICKVAL(Business_Use_of_Ticket__c , &apos;Charities&apos;), 
ISBLANK(Contact__c))) 
|| 
(AND(ISPICKVAL(Business_Use_of_Ticket__c , &apos;Charities&apos;), 
NOT(ISBLANK(Contact__c)), 
Contact__r.RecordType.DeveloperName&lt;&gt;&apos;Charity_Contact&apos;))</errorConditionFormula>
        <errorDisplayField>Contact__c</errorDisplayField>
        <errorMessage>For Charity tickets, a Charity Specific Contact MUST be selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Event_Time</fullName>
        <active>true</active>
        <errorConditionFormula>AND(!(ISBLANK (Event_Time__c)), NOT(REGEX( Event_Time__c ,
&quot;(\\d([0-1]|[1-9]):\\d([0-1]|[1-60]) (AM|am|PM|pm))&quot;)))</errorConditionFormula>
        <errorDisplayField>Event_Time__c</errorDisplayField>
        <errorMessage>Please enter validate Event time! for example: 11:23 AM</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>

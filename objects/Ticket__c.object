    <fields>
        <fullName>Assigned_To__c</fullName>
        <externalId>false</externalId>
        <formula>IF (assignToClient__c = TRUE, &quot;Client&quot;, 
IF (assignToEmployee__c = TRUE, &quot;Employee&quot;, 
IF (assignToProducer__c = TRUE, &quot;Producer&quot;, 
IF (assignToHost__c = TRUE, &quot;Host&quot;, 
IF (assignToCharity__c = TRUE, &quot;Charity&quot;, 
IF (assignToEmployeeIncentive__c = TRUE, &quot;Employee Incentive&quot;, 
IF (AssignToProspect__c = TRUE, &quot;Prospect&quot;, 
IF (AssignToVendor__c = TRUE, &quot;Vendor&quot;,&quot;&quot; ))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Assigned To</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>

    <validationRules>
        <fullName>At_least_1_Column_fee_must_be_populated</fullName>
        <active>true</active>
        <description>Throws error if all of the Column fee fields on the PEPM Fee record are blank</description>
        <errorConditionFormula>AND(
ISBLANK( Column_1_Fee__c ),
ISBLANK(Column_2_Fee__c ),
ISBLANK(Column_3_Fee__c ),
ISBLANK(Column_4_Fee__c )
)</errorConditionFormula>
        <errorMessage>You must populate at least 1 Column Fee field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Outpatient_Imaging_not_available_for_IL</fullName>
        <active>true</active>
        <description>Prevents users from adding Outpatient Imaging Management Services to rates related to IL ASOs</description>
        <errorConditionFormula>if( Rate__r.Secure_Information__r.Division__c  = &apos;IL&apos; &amp;&amp;   text(PEPM_Product_Service_Name__c) = &apos;Outpatient Imaging Management Services&apos;, true,false)</errorConditionFormula>
        <errorDisplayField>PEPM_Product_Service_Name__c</errorDisplayField>
        <errorMessage>Outpatient Imaging Management Services is not available in Illinois. Select a different product / service.</errorMessage>
    </validationRules>

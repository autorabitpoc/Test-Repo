        <formula>CASE(TEXT(PEPM_Product_Service_Name__c),
&quot;Other 1&quot;,&quot;Other: &quot;&amp;TEXT(Rate__r.Other_1_Service_Category__c)&amp;br()&amp;&quot;List Service:  &quot;&amp;Rate__r.Other_1_Service_Name__c,

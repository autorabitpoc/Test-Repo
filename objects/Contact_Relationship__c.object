    <fields>
        <fullName>Related_Record_On_Account__c</fullName>
        <description>Displays the related account or contact record name on the Contact Relationship related list on Account detail page</description>
        <externalId>false</externalId>
        <formula>HYPERLINK(
IF( Contact__c != null , &apos;/&apos;+ Contact__c , Related_Account__c ), 

IF( Contact__c != null , Contact__r.FirstName + &apos; &apos; + Contact__r.LastName , Related_Account__r.Name ), 

&apos;_blank&apos;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Displays the related account or contact record name on the Contact Relationship related list on Account detail page</inlineHelpText>
        <label>Related Record</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Related_Record__c</fullName>
        <description>Displays the related account, contact or user record name on the Contact Relationship related list on Contact detail page</description>
        <externalId>false</externalId>
        <formula>HYPERLINK(
IF( Account__c != null , &apos;/&apos;+ Account__c , IF( Related_Contact__c  != null , &apos;/&apos; + Related_Contact__c, &apos;/&apos; + Related_User__c )), 

IF( Account__c != null , Account__r.Name, IF( Related_Contact__c  != null , Related_Contact__r.FirstName + &apos; &apos; +  Related_Contact__r.LastName, Related_User__r.FirstName + &apos; &apos; + Related_User__r.LastName )), 

&apos;_blank&apos;)</formula>
        <inlineHelpText>Displays the related account, contact or user record name on the Contact Relationship related list on Contact detail page</inlineHelpText>
        <label>Related Record</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>

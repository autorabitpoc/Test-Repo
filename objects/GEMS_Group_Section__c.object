        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/connection.js&quot;)}
 
var url = parent.location.href;
var records = {!GETRECORDIDS($ObjectType.GEMS_Group_Section__c)};

if (records[0] == null) {
alert(&quot;Please select at least one group section to assign.&quot;);
} else {

var String = &apos;/apex/GEMSAssignBenefitAgreements?recs=&apos; + records.join(&apos;,&apos;);
parent.frames.location.replace(String);
}</url>

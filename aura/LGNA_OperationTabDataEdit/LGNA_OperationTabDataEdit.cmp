                        <span class="slds-truncate" title="Section Title">Group Type</span>
                    </button>
                </h3>                                   
            </div> 
            <div class="slds-show" aura:id="GroupTypeSection">                
                
                <lightning:layout horizontalAlign="space" verticalAlign="start" pullToBoundary="large" multipleRows="false">
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-truncate slds-input_bare">
                                <lightning:input type="checkbox" label="Critical Account" name="CriticalAccount" checked="{!v.editRecord.Critical_Account__c}"/>
                            </div>                                        
                        </div>
                    </lightning:layoutItem>
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-truncate slds-input_bare">
                                <div>
                                    <lightning:input type="checkbox" label="HCSC as Fiduciary" name="HCSCAsFiduciary" disabled="{!!v.accountCRUDMap.HCSC_as_Fiduciary__c}" checked="{!v.editRecord.HCSC_as_Fiduciary__c}"/>
                                </div>                                
                            </div>
                        </div>
                    </lightning:layoutItem>                            
                </lightning:layout> 
                <lightning:layout horizontalAlign="space" verticalAlign="start" pullToBoundary="large" multipleRows="false">
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-truncate slds-input_bare">
                                <lightning:input type="checkbox" label="Grandfathered Account?" name="GrandfatheredAccount" disabled="{!!v.accountCRUDMap.Grandfathered_Account__c}" checked="{!v.editRecord.Grandfathered_Account__c}"/>
                            </div>                                        
                        </div>
                    </lightning:layoutItem>
  <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">                        <div class="slds-col">
                         <div class="slds-truncate slds-input_bare">
                                
                           </div>
                        </div>
                    </lightning:layoutItem>                            
                </lightning:layout>
                <lightning:layout horizontalAlign="space" verticalAlign="start" pullToBoundary="large" multipleRows="false">
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-input_bare">
                                <!--<lightning:input label="COBRA Administration Type" name="COBRAAdministrationType" disabled="{!!v.accountCRUDMap.COBRA_Administration_Type__c}" value="{!v.editRecord.COBRA_Administration_Type__c}" />-->
                                <label class="slds-form-element__label">COBRA Administration Type</label><br/>                            
                                <ui:inputselect multiple="false" aura:id="cobratype" class="slds-select" disabled="{!!v.accountCRUDMap.COBRA_Administration_Type__c}" value="{!v.editRecord.COBRA_Administration_Type__c}"/>
                                
                            </div>                                        
                        </div>
                    </lightning:layoutItem>
                    
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-input_bare">
                                <div>
                                    <!--<lightning:input type="date" label="Grandfathered Date" name="GrandfatheredDate" value="{!v.editRecord.Grandfathered_Date__c}" />-->
                                    <label class="slds-form-element__label" for="date">Grandfathered Date</label>
                                    <ui:inputDate class="slds-input" disabled="{!!v.accountCRUDMap.Grandfathered_Date__c}" value="{!v.editRecord.Grandfathered_Date__c}" format="MM/dd/yyyy" displayDatePicker="true"/>								
                                </div>                                
                            </div>
                        </div>
                    </lightning:layoutItem>                            
                </lightning:layout>
                <lightning:layout horizontalAlign="space" verticalAlign="start" pullToBoundary="large" multipleRows="false">
                    
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-input_bare">
                                
                                <label class="slds-form-element__label">Premium Type</label><br/>   
                                <!-- <ui:inputselect multiple="true" aura:id="Premtype" class="slds-select" disabled="{!!v.accountCRUDMap.Premium_Type__c}" value="{!v.editRecord.Premium_Type__c}"/>-->
                                
                                <ui:inputselect multiple="true" aura:id="Premtype" class="slds-select" disabled="{!!v.hasModifyAll}" value="{!v.editRecord.Premium_Type__c}"/>
                                
                               <!-- <lightning:select name="Premtype" label="Premium Type" aura:id="Premtype" value="{!v.editRecord.Premium_Type__c}">
                                       <aura:iteration items="{!v.editRecord.Premium_Type__c}" var="option">
                                       <option text="{!option.label}" value="{!option.id}" selected="{!option.selected}"/>
                                      </aura:iteration>
                                  </lightning:select>  -->
                                <!--
                               <aura:if isTrue="{!v.hasModifyAll}" >
                                    <ui:inputselect multiple="true" aura:id="Premtype" class="slds-select" disabled="false" value="{!v.editRecord.Premium_Type__c}"/>
                                     <aura:set attribute="else"> 
                                       <lightning:input label="Premium Type" name="PremiumType" disabled="true" value="{!v.editRecord.Premium_Type__c}"/>
                                    </aura:set>
                                </aura:if>-->
                            </div>
                        </div>
                    </lightning:layoutItem>
                     <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-input_bare">
                                <!--<div>
                                    <lightning:input multiple="false" label="Funding Type" name="FundingType" value="{!v.editRecord.Funding_Type__c}" />                                    
                                </div>-->
                                
                              <label class="slds-form-element__label">Funding Type</label><br/>
                              <ui:inputselect multiple="true" aura:id="fundtype" class="slds-select" disabled="{!!v.hasModifyAll}" value="{!v.editRecord.Funding_Type__c}"/>
                           
                            </div>

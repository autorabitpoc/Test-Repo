                        <lightning:icon class="slds-show" aura:id="BlueSTARTeamInformationSection" iconName="utility:chevrondown" size="x-small"/>                        
                        <lightning:icon class="slds-hide" aura:id="BlueSTARTeamInformationSection" iconName="utility:chevronright" size="x-small"/>

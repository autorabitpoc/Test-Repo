                                    <ui:inputselect multiple="false" aura:id="lineOfBusiness" class="slds-select" disabled="{!!v.hasModifyAll}" value="{!v.editRecord.Line_of_business__c}"/>
                                     
                                    
                                </div>                                
                            </div>
                        </div>
                    </lightning:layoutItem>					
                </lightning:layout>
            </div> 
            
            <!--Starts Display Group Type -->             

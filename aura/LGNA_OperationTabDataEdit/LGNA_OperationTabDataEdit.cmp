                                  <div style="max-width: 400px;">
        								<lightning:inputAddress
            								addressLabel="HQ Address"
            									streetLabel="Street"
            									cityLabel="City"
            									countryLabel="Country"
            									provinceLabel="Privince"
            									postalCodeLabel="Postal Code"
            									street="{!v.editRecord.BillingStreet}"
            									city="{!v.editRecord.BillingCity}"
           									    country="{!v.editRecord.BillingCountry}"
            									province="{!v.editRecord.BillingState}"
            									postalCode="{!v.editRecord.BillingPostalCode}"
            									
            									
                                         />
                                   </div>
                                 <aura:set attribute="else">
                                     <label class="slds-form-element__label" for="input-04">HQ Address</label><br/>
                                     <lightning:formattedAddress street="{!v.editRecord.ShippingStreet}"
                                                            city="{!v.editRecord.ShippingCity}"
                                                            country="{!v.editRecord.ShippingCountry}"
                                                            province="{!v.editRecord.ShippingState}"

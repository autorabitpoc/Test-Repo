                        <div class="slds-col">
                            <div class="slds-input_bare">
                                <!--<lightning:input readonly="true" type="date" label="Original Effective Date" name="OriginalEffectiveDate"  value="{!v.editRecord.Effective_Date__c}" />-->
                                <label class="slds-form-element__label" for="date">Original Effective Date</label>
                                 <ui:inputDate class="slds-input" disabled ="{!!v.hasModifyAll}" value="{!v.editRecord.Effective_Date__c}" format="MM/dd/yyyy" displayDatePicker="true"/>  
                            </div>                                        
                        </div>                                                         
                        
                    </lightning:layoutItem>
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-input_bare">
                                <lightning:input label="Anniversary Date" name="AnniversaryDate" disabled="{!!v.accountCRUDMap.Group_Anniversary_Date__c}"  value="{!v.editRecord.Group_Anniversary_Date__c}" />                              
                            </div>
                        </div>
                    </lightning:layoutItem>                            
                </lightning:layout>
                <lightning:layout horizontalAlign="space" verticalAlign="start" pullToBoundary="large" multipleRows="false">
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-input_bare">
                              <!-- <lightning:input type="date" label="Contract End" disabled="{!!v.accountCRUDMap.Contract_End__c}" name="ContractEnd" value="{!v.editRecord.Contract_End__c}" />-->
                                <label class="slds-form-element__label" for="date">Contract End</label>
                                <ui:inputDate class="slds-input" disabled="{!!v.accountCRUDMap.Contract_End__c}" value="{!v.editRecord.Contract_End__c}" format="MM/dd/yyyy" displayDatePicker="true"/>                                                                                
                               
                              </div>                                        
                        </div>
                    </lightning:layoutItem>
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-input_bare">
                                <c:LightningLookup startingName="{!v.editRecord.Contract_Signer__r.Name}" sObjectName="Contact" sObjectField="Account.Contract_Signer__c" displayedFieldName="Name" valueFieldName="Id" cmpId="ContractRecipient" label="Contract Recipient" selectedValue="{!v.editRecord.Contract_Signer__c}" whereClause="{!'RecordType.Name = \'Group Contact\' AND AccountId = ' + '\'' +  v.recordId + '\''}" showHelp="true" helpText="This Contact must be related to this Account."/>                                                        
                            </div>
                        </div>
                    </lightning:layoutItem>                            
                </lightning:layout>
                <lightning:layout horizontalAlign="space" verticalAlign="start" pullToBoundary="large" multipleRows="false">
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-input_bare">
                               <!-- <lightning:input type="date" label="Open Enrollment End Date" name="OpenEnrollmentEndDate" value="{!v.editRecord.Open_Enrollment_End_Date__c}" />-->
                                <label class="slds-form-element__label" for="date">Open Enrollment End Date</label>
                                <ui:inputDate class="slds-input" disabled="{!!v.accountCRUDMap.Open_Enrollment_End_Date__c}" value="{!v.editRecord.Open_Enrollment_End_Date__c}" format="MM/dd/yyyy" displayDatePicker="true"/>
                                
                              </div>                                        
                        </div>
                    </lightning:layoutItem>
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-input_bare">
                               <!-- <lightning:input type="date" label="Open Enrollment Start Date" disabled="{!!v.accountCRUDMap.Open_Enrollment_Date__c}"  value="{!v.editRecord.Open_Enrollment_Date__c}" />-->
                                <label class="slds-form-element__label" for="date">Open Enrollment Start Date</label>
                                <ui:inputDate class="slds-input" disabled="{!!v.accountCRUDMap.Open_Enrollment_Date__c}" value="{!v.editRecord.Open_Enrollment_Date__c}" format="MM/dd/yyyy" displayDatePicker="true"/>								
                               
                              </div>
                        </div>
                    </lightning:layoutItem>                            
                </lightning:layout>
                <lightning:layout horizontalAlign="space" verticalAlign="start" pullToBoundary="large" multipleRows="false">
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-input_bare">
                                <!--<lightning:input type="date" label="Renewal Date" name="RenewalDate" value="{!v.editRecord.Renewal_Date__c}" />-->
                                <label class="slds-form-element__label" for="date">Renewal Date</label>
                                <ui:inputDate class="slds-input" disabled="{!!v.accountCRUDMap.Renewal_Date__c}" value="{!v.editRecord.Renewal_Date__c}" format="MM/dd/yyyy" displayDatePicker="true"/>								
                            </div>                                        
                        </div>
                    </lightning:layoutItem>
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-truncate slds-input_bare">
                                <div>

                        <div class="slds-col">
                            <div class="slds-truncate slds-input_bare">
                                <aura:if isTrue="{!v.hasModifyAll}" >
                                  <div style="max-width: 400px;">
        								<lightning:inputAddress
            								addressLabel="Billing Address"
            									streetLabel="Street"
            									cityLabel="City"
            									countryLabel="Country"
            									provinceLabel="Privince"
            									postalCodeLabel="Postal Code"

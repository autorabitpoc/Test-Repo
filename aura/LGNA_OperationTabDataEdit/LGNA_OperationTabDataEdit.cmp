                        <div class="slds-col">
                            <div class="slds-truncate slds-input_bare">
                                <div>
                                  <lightning:input label="Billing County" name="BillingCounty" disabled="{!!v.hasModifyAll}"  value="{!v.editRecord.Billing_County__c}" />                                                        
                                 
                                    <!--
                                   <aura:if isTrue="{!v.hasModifyAll}" >
                                     <lightning:input label="Billing County" name="BillingCounty" disabled="false"  value="{!v.editRecord.Billing_County__c}" />                                                        
                                        <aura:set attribute="else">
                                         <lightning:input label="Billing County" name="BillingCounty" disabled="true" value="{!v.editRecord.Billing_County__c}" />                                                        
                                       </aura:set>
                                    </aura:if>-->
                                </div>                                
                            </div>
                        </div>
                    </lightning:layoutItem>                            
                </lightning:layout>

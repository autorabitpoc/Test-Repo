                            <div class="slds-truncate slds-input_bare">                                    
                                <lightning:input type="number" label="Account Cluster Number" name="AccountClusterEffectiveDate" value="{!v.viewRecord.Account_Cluster_Number__c}" />                                                                      
                            </div>
                        </div>
                    </lightning:layoutItem>                            
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-truncate slds-input_bare">                                    
                                <lightning:input label="Account Cluster User Identification" name="AccountClusterUserIdentification" value="{!v.viewRecord.Account_Cluster_User_Identification__c}" />                                                                       
                            </div>
                        </div>
                    </lightning:layoutItem>                            
                </lightning:layout>                    
                <lightning:layout horizontalAlign="space" verticalAlign="start" pullToBoundary="large" multipleRows="false">
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-truncate slds-input_bare">
                                <lightning:input label="Account Executive 1" name="AccountExecutive1" value="{!v.viewRecord.Account_Cluster_Name__c}" />                                    
                            </div>                                        
                        </div>
                    </lightning:layoutItem>
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-truncate slds-input_bare">
                                <lightning:input label="Account Executive 2" name="AccountExecutive2" value="{!v.viewRecord.Account_Executive_2__c}" />                                                                                                                                               
                            </div>
                        </div>
                    </lightning:layoutItem>                            
                </lightning:layout>
                <!---  cluster-->
                 <lightning:layout horizontalAlign="space" verticalAlign="start" pullToBoundary="large" multipleRows="false">
                      <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-truncate slds-input_bare">
                                <lightning:input label="Sales Cluster Number" name="SalesClusterNumber" value="{!v.viewRecord.Sales_Cluster_Number__c}" />                                                                                                                                               
                            </div>
                        </div>
                    </lightning:layoutItem>    
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-input_bare">                                    
                             
                            </div>
                        </div>
                    </lightning:layoutItem>                            
                </lightning:layout>              
                <!---cluster -->
                <lightning:layout horizontalAlign="space" verticalAlign="start" pullToBoundary="large" multipleRows="false">
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-input_bare">
                                <!--<lightning:input readonly="true" type="date" label="Underwriter End Date" name="UnderwriterEndDate" value="{!v.viewRecord.Underwriter_End_Date__c}" />-->
                                <label class="slds-form-element__label" for="date">Underwriter End Date</label>
                                
                                <ui:inputDate class="slds-input" value="{!v.viewRecord.Underwriter_End_Date__c}" format="MM/dd/yyyy" displayDatePicker="true"/>												

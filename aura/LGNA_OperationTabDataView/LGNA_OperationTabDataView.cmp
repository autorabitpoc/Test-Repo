                        <div class="slds-col">
                            <div class="slds-truncate slds-input_bare">
                                <lightning:textarea label="Sales Executive" name="SalesExecutive" value="{!v.viewRecord.Sales_Team__c}" />
                            </div>                                        
                        </div>
                    </lightning:layoutItem>
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-input_bare">
                                <!--<lightning:input readonly="true" type="date" label="Account Cluster Effective Date" name="AccountClusterEffectiveDate" value="{!v.viewRecord.Account_Cluster_Effective_Date__c}" />-->
                                
                                <label class="slds-form-element__label" for="date">Account Cluster Effective Date</label>
                                
                                <ui:inputDate class="slds-input" value="{!v.viewRecord.Account_Cluster_Effective_Date__c}" format="MM/dd/yyyy" displayDatePicker="true"/>								  
                            </div>
                        </div>
                    </lightning:layoutItem>                            
                </lightning:layout>                    
                <lightning:layout horizontalAlign="space" verticalAlign="start" pullToBoundary="large" multipleRows="false"> 
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">

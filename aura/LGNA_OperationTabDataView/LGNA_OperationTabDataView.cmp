                </lightning:layout>                    
                <lightning:layout horizontalAlign="space" verticalAlign="start" pullToBoundary="large" multipleRows="false">
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-truncate slds-input_bare">
                                <lightning:input type="checkbox" label="Grandfathered Account?" name="GrandfatheredAccount" checked="{!v.viewRecord.Grandfathered_Account__c}"/>
                            </div>                                        
                        </div>
                    </lightning:layoutItem>
                    
                     <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                           
                        </div>
                    </lightning:layoutItem>    
                           
                </lightning:layout>                    
                <lightning:layout horizontalAlign="space" verticalAlign="start" pullToBoundary="large" multipleRows="false">
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-truncate slds-input_bare">
                                <lightning:input label="COBRA Administration Type" name="COBRAAdministrationType" value="{!v.viewRecord.COBRA_Administration_Type__c}" />                                    
                            </div>                                        
                        </div>
                    </lightning:layoutItem>
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-input_bare">
                                <!--<lightning:input readonly="true" type="date" label="Grandfathered Date" name="GrandfatheredDate" value="{!v.viewRecord.Grandfathered_Date__c}" />-->
                                <label class="slds-form-element__label" for="date">Grandfathered Date</label>
                                
                                <ui:inputDate class="slds-input" value="{!v.viewRecord.Grandfathered_Date__c}" format="MM/dd/yyyy" displayDatePicker="true"/>
                            </div>
                        </div>
                    </lightning:layoutItem>  
                    
                </lightning:layout>  
                <lightning:layout horizontalAlign="space" verticalAlign="start" pullToBoundary="large" multipleRows="false">
                    
                    
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-truncate slds-input_bare">
                                <lightning:input label="Premium Type" name="PremiumType" value="{!v.viewRecord.Premium_Type__c}" />                                    
                            </div>
                        </div>
                    </lightning:layoutItem>   
                       <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-truncate slds-input_bare">
                                <lightning:input label="Funding Type" name="FundingType" value="{!v.viewRecord.Funding_Type__c}" />                                    
                            </div>
                        </div>
                    </lightning:layoutItem>              				
                </lightning:layout>  					
            </div>
            
            <!--Starts Display Address Information -->             
            <div class="slds-is-open slds-section" onclick="{!c.AddressInformationSectionClick}">
                <h3 class="slds-section__title">
                    <button class="slds-button slds-section__title-action">                        
                        <lightning:icon class="slds-show" aura:id="AddressInformationSection" iconName="utility:chevrondown" size="x-small"/>                        
                        <lightning:icon class="slds-hide" aura:id="AddressInformationSection" iconName="utility:chevronright" size="x-small"/>
                        <span class="slds-truncate" title="Section Title">Address Information</span>
                    </button>
                </h3>                                   
            </div> 
            <div class="slds-show" aura:id="AddressInformationSection">    
                <lightning:layout horizontalAlign="space" verticalAlign="start" pullToBoundary="large" multipleRows="false">
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-truncate slds-input_bare">
                                <label class="slds-form-element__label" for="input-04">Billing Address</label><br/>
                                <lightning:formattedAddress street="{!v.viewRecord.BillingStreet}"
                                                            city="{!v.viewRecord.BillingCity}"
                                                            country="{!v.viewRecord.BillingCountry}"
                                                            province="{!v.viewRecord.BillingState}"
                                                            postalCode="{!v.viewRecord.BillingPostalCode}"
                                                            />                                                               
                            </div>                                        
                        </div>
                    </lightning:layoutItem>
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-truncate slds-input_bare">
                                <lightning:input label="Billing County" name="BillingCounty" value="{!v.viewRecord.Billing_County__c}" />                                                                       
                            </div>
                        </div>
                    </lightning:layoutItem>                            
                </lightning:layout>                    
                <lightning:layout horizontalAlign="space" verticalAlign="start" pullToBoundary="large" multipleRows="false">
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-truncate slds-input_bare">
                                <label class="slds-form-element__label" for="input-04">HQ Address</label><br/>
                                <lightning:formattedAddress street="{!v.viewRecord.ShippingStreet}"
                                                            city="{!v.viewRecord.ShippingCity}"
                                                            country="{!v.viewRecord.ShippingCountry}"
                                                            province="{!v.viewRecord.ShippingState}"
                                                            postalCode="{!v.viewRecord.ShippingPostalCode}"
                                                            />   
                            </div>                                        
                        </div>
                    </lightning:layoutItem>
                    <lightning:layoutItem flexibility="auto" padding="horizontal-medium" largeDeviceSize="6" mediumDeviceSize="4" smallDeviceSize="3" size="12">
                        <div class="slds-col">
                            <div class="slds-truncate slds-input_bare">
                                <lightning:input label="Key Locations" name="KeyLocations" value="{!v.viewRecord.Key_Locations__c}" />                                                                                                            
                            </div>
                        </div>
                    </lightning:layoutItem>                            
                </lightning:layout>
            </div>
            

                            <div class="slds-truncate slds-input_bare">
                                <lightning:input type="checkbox" label="HCSC as Fiduciary" name="HCSCAsFiduciary" checked="{!v.viewRecord.HCSC_as_Fiduciary__c}"/>
                            </div>
                        </div>
